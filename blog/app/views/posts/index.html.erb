<div class="tab"><h1>Recent Posts</h1></div>
<div class="jumbotron p-3 p-md-5 text-white rounded bg-dark">
  <%= image_tag("cfq.png", :alt => "FAQ") %>
       
      </div>
<% @posts.each do |post| %>
	<div class="well">
	<h3><%= post.title %></h3>
	<p><%= post.body %></p>
	<%= link_to "View Full Post", post_path=(post), :class => 'btn btn-default' %>
	</div>
<% end %>